    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0-rc.6",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "phantomjs-prebuilt": "^2.1.7",
    "react-intl": "^2.4.0",
    "react-test-renderer": "^16.4.2",
    "sass-loader": "^7.1.0",
    "satisfied": "^1.1.2",
    "sinon": "^5.0.10",
    "sinon-chai": "^3.1.0",
    "storybook-readme": "^5.0.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "publishConfig": { "registry": "https://npm.pkg.github.com/" }
}
